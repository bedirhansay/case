{
  "compilerOptions": {
    "target": "es2015",
    "types": ["@testing-library/jest-dom"],
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "jest.config.ts",
    "__tests__/app/Home.test.jsx"
, "__tests__/FormatDate.jsx", "src/container/__test__/ChekcoutPage.test.tsx", "__tests__/app/RootLayout.tsx", "src/components/__test__/pagination.test.tsx"  ],
  "exclude": ["node_modules"]
}
